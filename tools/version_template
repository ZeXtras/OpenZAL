<target name="zal-${ZimbraVersion}"
        description="Create jar for Zimbra ${ZimbraVersion}"
        depends="init">
  <build-zal ZimbraVersion="${ZimbraVersion}"
             MajorZimbraVersion = "${MajorZimbraVersion}"
             MiddleZimbraVersion = "${MiddleZimbraVersion}"
             MinorZimbraVersion = "${MinorZimbraVersion}" />
</target>


<target name="zal-${ZimbraVersion}-itbase"
        description="Create test jar for Zimbra ${ZimbraVersion}"
        depends="init">
  <build-zal-itbase ZimbraVersion="${ZimbraVersion}"
                    MajorZimbraVersion = "${MajorZimbraVersion}"
                    MiddleZimbraVersion = "${MiddleZimbraVersion}"
                    MinorZimbraVersion = "${MinorZimbraVersion}" />
</target>
